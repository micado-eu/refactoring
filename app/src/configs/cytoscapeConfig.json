{
  "style": [
    {
      "selector": "node",
      "style": {
        "background-color": "#666",
        "label": "data(title)"
      }
    }, {
      "selector": "edge",
      "style": {
        "width": 3,
        "line-color": "#f00",
        "target-arrow-color": "#000",
        "target-arrow-shape": "triangle"
      }
    }
  ],
  "layout": {
    "name": "grid",
    "rows": 1
  },
  "autolock": true,
  "panningEnabled": false
}
